<!DOCTYPE html>
<html>
<head>
  <title>All Content</title>
  <link rel="stylesheet" href="/path/to/your/css">
</head>
<body>
  <h1>All Content</h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Description</th>
        <!-- Add more columns as needed -->
      </tr>
    </thead>
    <tbody>
      <% resources.forEach(resource => { %>
        <tr>
          <td><%= resource.id %></td>
          <td><%= resource.name %></td>
          <td><%= resource.URL %></td>
          <!-- Add more fields as needed -->
        </tr>
      <% }) %>
    </tbody>
  </table>
  <a href="/home">Back to Home</a>
</body>
</html>

